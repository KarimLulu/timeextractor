{
  "name": "temporal extractor",
  "manifest_version": 2, 
  "version": "1",
  "description": "Extracts temporal information from html page",
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "content_scripts": [ {
    "js": [ "jquery.min.js", "foundation.js", "foundation.tooltip.js", "jquery.ba-replacetext.min.js", "options.js", "rules.js", "main.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }],
     "background": {
    "scripts": ["background.js" ],
    "persistent": false
  },
  "permissions": [
   "storage",
    "tabs", "http://*/*", "https://*/*"],
    "options_page": "options.html"
  
}